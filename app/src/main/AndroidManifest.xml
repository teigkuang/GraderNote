<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tearhan.graderform">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/tiger_grader"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/tiger_grader"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".activity.FormListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.FormDetailActivity" />
        <activity android:name=".activity.FormEditActivity" />
    </application>

</manifest>